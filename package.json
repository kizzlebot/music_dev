{
  "name": "music_dev",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config build_config/webpack.server.js",
    "build-client": "webpack --colors --display-error-details --config build_config/webpack.client.js",
    "build": "NODE_ENV=production concurrently \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config build_config/webpack.server-watch.js",
    "watch-server-start": "just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config build_config/webpack.client-watch.js --port=${WEBPACK_PORT:=\"8080\"}",
    "watch": "NODE_ENV=development PORT=8000 concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "test": "NODE_ENV=test mocha --reporter mocha-unfunk-reporter --timeout 10000 --compilers js:babel-register --require test/bootstrap/node test/*.test.js"
  },
  "dependencies": {
