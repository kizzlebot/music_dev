{
  "name": "music_dev",
  "version": "1.0.0",
  "description": "Boilerplate project for building Isomorphic apps using React and Redux",
  "scripts": {
    "test": "npm run test:client && npm run test:server",
    "test:client": "NODE_ENV=test mocha shared/tests/*.spec.js --compilers js:babel-register --require shared/tests/bootstrap/node.js --recursive shared/tests/*.js",
    "test:server": "cross-env NODE_ENV=test PORT=8080 mocha --reporter mocha-unfunk-reporter --timeout 5000 --compilers js:babel-register --require server/tests/bootstrap/node.js --recursive server/tests/**/*.spec.js",
    "test:watch": "npm run test:client -- --watch",

    "start": "cross-env NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "start:quiet": "cross-env QUIET=1 NODE_ENV=development nodemon index.js",
    "production": "npm run start:prod",
    "bs": "npm run clean && npm run build && npm run start:prod",
    "minify": "cleancss -o static/css/app.min.css static/css/app.css",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js && npm run minify",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint client server shared"
  },
  "pre-commit": [
    "lint",
    "test",
    "test:server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kizzlebot/music_dev.git"
  },
  "bugs": {
