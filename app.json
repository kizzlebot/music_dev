{
  "name": "music_dev",
  "description": "Connects to various music apis and lets you use its services in you music-listening experience",
  "scripts": {
  },
  "env": {
    "FACEBOOK_ID": {
      "required": true
    },
    "FACEBOOK_SECRET": {
      "required": true
    },
    "GITHUB_ID": {
      "required": true
    },
    "GITHUB_SECRET": {
      "required": true
    },
    "GOOGLE_ID": {
      "required": true
    },
    "GOOGLE_SECRET": {
      "required": true
    },
    "HOMEBREW_GITHUB_API_TOKEN": {
      "required": true
    },
    "HOST": {
      "required": true
    },
    "HOSTNAME": {
      "required": true
    },
    "IMGUR_API_ID": {
      "required": true
    },
    "IMGUR_API_SECRET": {
      "required": true
    },
    "IMGUR_CLIENT_ID": {
      "required": true
    },
    "IMGUR_CLIENT_SECRET": {
      "required": true
    },
    "INSTAGRAM_ID": {
      "required": true
    },
    "INSTAGRAM_SECRET": {
      "required": true
    },
    "LASTFM_KEY": {
      "required": true
    },
    "LASTFM_SECRET": {
      "required": true
    },
    "LINKEDIN_ID": {
      "required": true
    },
    "LINKEDIN_SECRET": {
      "required": true
    },
    "MONGODB": {
      "required": true
    },
    "MONGOLAB_URI": {
      "required": true
    },
    "NODE_ENV": {
      "required": true
    },
