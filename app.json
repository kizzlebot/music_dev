    "SOUNDCLOUD_ID": {
      "required": true
    },
    "SOUNDCLOUD_SECRET": {
      "required": true
    },
    "TWITTER_KEY": {
      "required": true
    },
    "TWITTER_SECRET": {
      "required": true
    }
